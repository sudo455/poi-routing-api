# Production environment variables
# Copy to .env and fill in actual values

# Database
DB_PASSWORD=your-secure-db-password-here

# JWT Secret (generate with: python -c "import secrets; print(secrets.token_hex(32))")
JWT_SECRET_KEY=your-secure-jwt-secret-here

# Tailscale (for CI/CD deployment)
# Create OAuth client at: https://login.tailscale.com/admin/settings/oauth
TS_OAUTH_CLIENT_ID=your-tailscale-client-id
TS_OAUTH_SECRET=your-tailscale-secret

# Deployment target
DEPLOY_USER=your-ssh-user
DEPLOY_HOST=your-server-hostname
